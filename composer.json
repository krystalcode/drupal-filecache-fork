{
    "name": "drupal/filecache",
    "description": "A cache backend that uses the file system for storage.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://drupal.org/project/filecache",
    "authors": [
        {
            "name": "Ognyan Kulev (@ogi)",
            "homepage": "https://www.drupal.org/u/ogi",
            "role": "Maintainer"
        },
        {
            "name": "Pieter Frenssen (@pfrenssen)",
            "homepage": "https://www.drupal.org/u/pfrenssen",
            "role": "Co-maintainer"
        }
    ],
    "support": {
        "issues": "https://drupal.org/project/issues/filecache",
        "source": "https://cgit.drupalcode.org/filecache"
    },
    "require": {
        "drupal/core": "~8.5"
    },
    "require-dev": {
        "composer/installers": "^1.2",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/coder": "^8.3.1",
        "webflo/drupal-core-require-dev": "~8.5"
    },
    "extra": {
        "installer-paths": {
            "../build/core": ["type:drupal-core"],
            "../build/modules/contrib/{$name}": ["type:drupal-module"],
            "../build/themes/contrib/{$name}": ["type:drupal-theme"]
        }
    }
}
